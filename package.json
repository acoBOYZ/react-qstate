{
  "name": "@acoboyz/react-qstate",
  "version": "0.2.0",
  "description": "QueryHook for managing, caching and syncing asynchronous and remote data in React powered by @tanstack/react-query",
  "author": "acoBOYZ",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/acoBOYZ/react-qstate.git"
  },
  "homepage": "https://github.com/acoBOYZ",
  "funding": {
    "type": "github",
    "url": "https://github.com/acoBOYZ"
  },
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build && publint --strict",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "release": "tanstack-config publish"
  },
  "keywords": [
    "react",
    "react-query",
    "superjson",
    "state management",
    "hooks"
  ],
  "dependencies": {
    "@tanstack/react-query": "^5.59.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "superjson": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-react": "^7.25.7",
    "@babel/preset-typescript": "^7.25.7",
    "@tanstack/config": "^0.13.4",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^8.57.1",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "glob": "^11.0.0",
    "publint": "^0.2.11",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^4.2.3"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0"
  }
}
